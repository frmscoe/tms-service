{
  "name": "data-preparation",
  "version": "1.1.0",
  "description": "data-preparation",
  "main": "src/index.ts",
  "types": "build/index.d.ts",
  "scripts": {
    "dev": "nodemon -r dotenv/config",
    "build": "tsc --project tsconfig.json",
    "start": "node -r dotenv/config build/index.js",
    "cleanup": "rm -rf dist template jest.config.js jest.config.js.map node_modules",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "test": "jest --config=jest.config.ts --passWithNoTests",
    "test:watch": "jest --config=jest.config.ts --watch"
  },
  "keywords": [
    "mojaloop",
    "actio",
    "frm"
  ],
  "contributors": [
    {
      "name": "Len Bekker"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@log4js-node/logstash-http": "^1.1.0",
    "arangojs": "^8.1.0",
    "axios": "^1.3.1",
    "dotenv": "^16.0.3",
    "elastic-apm-node": "^3.42.0",
    "ioredis": "^5.3.0",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-router": "^12.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.7.1",
    "node-cache": "^5.1.2",
    "swagger2-koa": "^4.0.0"
  },
  "devDependencies": {
    "@types/arangodb": "^3.5.14",
    "@types/axios": "^0.14.0",
    "@types/elastic-apm-node": "^2.7.0",
    "@types/eslint": "^8.4.10",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^29.4.0",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-router": "^7.4.4",
    "@types/lodash": "^4.14.191",
    "@types/log4js": "^2.3.5",
    "@types/node": "^18.11.18",
    "@types/nodemon": "^1.19.2",
    "@types/prettier": "^2.7.2",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "redis-mock": "^0.56.3",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "tplant": "^3.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}